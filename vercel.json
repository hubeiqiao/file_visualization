{
  "version": 2,
  "functions": {
    "server.py": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/**/*.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
} 